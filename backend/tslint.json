{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "completed-docs": [
        true,
        {
          "enums": true,
          "functions": {
            "visibilities": [
              "exported"
            ]
          },
          "interfaces": {
            "visibilities": [
              "exported"
            ]
          },
          "methods": true,
          "types": {
            "visibilities": [
              "exported"
            ]
          },
          "variables": {
            "visibilities": [
              "exported"
            ]
          }
        }
      ]
    },
    "rulesDirectory": []
}
